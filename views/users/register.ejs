	<!--================Register Box Area =================-->
	<section class="login_box_area section_gap">
		<div class="container">
				<div class="col-lg-12">
					<div class="login_form_inner">
						<h3>Register</h3>
						<form class="row login_form" id="signup" method="post" novalidate="novalidate">
              <div class="col-md-12 form-group">
								<input type="text" class="form-control" id="fname" name="fname" placeholder="First Name" onfocus="this.placeholder = ''" onblur="this.placeholder = 'First Name'">
								<p id="err" class="text-danger"></p>
							</div>
              <div class="col-md-12 form-group">
								<input type="text" class="form-control" id="lname" name="lname" placeholder="Last Name" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Last Name'">
								<p id="err" class="text-danger"></p>

							</div>
							<div class="col-md-12 form-group">
								<input type="text" class="form-control" id="email" name="email" placeholder="Email ID" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Email'">
								<p id="err" class="text-danger"></p>

							</div>
              <div class="col-md-12 form-group">
								<input type="number" class="form-control" id="mno" name="mno" placeholder="Mobile Number" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Mobile'">
								<p id="err" class="text-danger"></p>

							</div>
							<div class="col-md-12 form-group">
								<input type="password" class="form-control" id="password" name="password" placeholder="Password" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Password'">
								<p id="err" class="text-danger"></p>

							</div>
							<div class="col-md-12 form-group">
								<input type="password" class="form-control" id="confPassword" name="confPassword" placeholder="Confirm Password" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Confirm Password'">
								<p id="err" class="text-danger"></p>
							</div>
						
							<div class="col-md-12 form-group">
								<button type="submit" value="submit" class="primary-btn" >Register</button>
							
							</div>
						</form>
            <%=(typeof message!=='undefined')?message:""%>
					</div>
				</div>
			</div>
		</div>
	</section>
	<!--================End Register Box Area =================-->

